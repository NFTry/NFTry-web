    "date-fns": "^2.30.0",
    "dotenv": "^16.1.3",
    "dotenv-cli": "^7.2.1",
    "immer": "^10.0.2",
    "lodash-es": "^4.17.21",
    "lottie-web": "^5.12.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
